<body>
    <div class="interface">
        <div class="div1">
            <div class="leftImage">
                <img src="landing-2x.png" alt="">
            </div>
            <div class="loginBox">
                <form action="/login" method="post">
                    <img src="instagramtext.webp" id="instaText" alt="">
                    <input type="text" name="ID" id="username" placeholder="Phone number, username, or email"
                        required>
                    <input type="password" name="password" id="userPassword" placeholder="Password">
                    <input type="submit" value="Log in">
                    <p id="or">----------------------&nbsp;&nbsp;&nbsp;OR&nbsp;&nbsp;&nbsp;----------------------</p>
                    <a href="www.facebook.com" id="fb">
                        <img src="facebook.png" id="facebookicon">
                        <p id="facebookLogin">Log in with Facebook</p>
                    </a>
                    <a href="" id="forgotPassword">
                        <p id="forgotPassword">Forgot password?</p>
                    </a>
                    <p id="signup">Don't have an account? <a href="/signUp">Sign up</a></p>
                </form>
            </div>
        </div>
        <div class="div2">
